{
  "extends": [
    "stylelint-prettier",
    "stylelint-config-standard",
    "stylelint-config-standard-scss",
    "stylelint-config-idiomatic-order"
  ],
  "plugins": [
    "stylelint-scss",
    "stylelint-order"
  ],
  "rules": {
    "rule-empty-line-before": null,
    "selector-pseudo-class-no-unknown": null,
    "no-descending-specificity": null
  }
}